{
	"$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"files": {
		"include": [
			"src/**/*.ts",
			"test/**"
		],
		"ignore": [
			"dist/**",
			"node_modules/**",
			"prisma/**"
		]
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	}
}