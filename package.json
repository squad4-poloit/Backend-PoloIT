{
  "name": "backend-poloit",
  "version": "1.0.0",
  "main": "src/index.ts",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "build": "tsc && tsc-alias -p tsconfig.json",
    "build:clean": "rimraf dist",
    "start": "dotenv -- node -r tsconfig-paths/register dist/src/app.js",
    "dev": "dotenv -e .env.dev --  tsnd -r tsconfig-paths/register --respawn src/app.ts",
    "local": "dotenv -e .env.local --  tsnd -r tsconfig-paths/register --respawn src/app.ts",
    "test": "dotenv -- jest",
    "test:dev": "dotenv -e .env.dev -- jest",
    "test:local": "dotenv -e .env.local -- ts-node jest",
    "prisma-migrate:deploy": "dotenv -- prisma migrate deploy --schema ./prisma/schema.prisma",
    "prisma-migrate:local": "dotenv -e .env.local -- prisma migrate dev --schema ./prisma/local/schema.local.prisma",
    "prisma-migrate:dev": "dotenv -e .env.dev -- prisma migrate dev --schema ./prisma/schema.prisma",
    "prisma-reset:local": "dotenv -e .env.local --  prisma migrate reset --schema ./prisma/local/schema.local.prisma",
    "prisma-reset:dev": "dotenv -e .env.dev --  prisma migrate reset",
    "prisma-studio:prod": "dotenv -e .env -- prisma studio --schema ./prisma/local/schema.local.prisma",
    "prisma-studio:dev": "dotenv -e .env.dev -- prisma studio --schema ./prisma/local/schema.local.prisma",
    "prisma-studio:local": "dotenv -e .env.local -- prisma studio --schema ./prisma/local/schema.local.prisma",
    "prisma-studio:test": "dotenv -e .env.test -- prisma studio --schema ./prisma/local/schema.local.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^5.19.0",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "express": "4.19.2",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "supertest": "^7.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@jest/globals": "^29.7.0",
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.21",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "22.4.0",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "morgan": "1.10.0",
    "prisma": "^5.19.0",
    "prisma-dbml-generator": "^0.12.0",
    "rimraf": "6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  }
}
