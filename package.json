{
  "name": "backend-poloit",
  "version": "1.0.0",
  "main": "src/index.ts",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "build": "tsc && tsc-alias -p tsconfig.json",
    "build:clean": "rimraf dist",
    "start": "dotenv -- node -r tsconfig-paths/register dist/src/index.js",
    "dev": "dotenv -e .env.dev --  tsnd -r tsconfig-paths/register --respawn src/index.ts",
    "local": "dotenv -e .env.local --  tsnd -r tsconfig-paths/register --respawn src/index.ts",
    "test": "dotenv -- jest",
    "test:dev": "dotenv -e .env.dev -- jest",
    "test:local": "dotenv -e .env.local -- jest",
    "prisma-migrate:deploy": "dotenv -- prisma migrate deploy --schema ./prisma/schema.prisma",
    "prisma-migrate:local": "dotenv -e .env.local -- prisma migrate dev --schema ./prisma/local/schema.local.prisma",
    "prisma-migrate:dev": "dotenv -e .env.dev -- prisma migrate dev --schema ./prisma/schema.prisma",
    "prisma-seed:local": "dotenv -e .env.local --  prisma db seed",
    "prisma-seed:dev": "dotenv -e .env.dev --  prisma db seed",
    "prisma-studio:local": "dotenv -e .env.local -- prisma studio --schema ./prisma/local/schema.local.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^5.19.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "express": "4.19.2",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "supertest": "^7.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/bcrypt": "5.0.2",
    "@types/express": "4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "9.0.6",
    "@types/node": "22.4.0",
    "@types/supertest": "^6.0.2",
    "morgan": "1.10.0",
    "prisma": "^5.19.0",
    "rimraf": "6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  }
}