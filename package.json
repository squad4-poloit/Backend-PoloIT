{
  "name": "backend-poloit",
  "version": "1.0.0",
  "main": "src/index.ts",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "lint": "biome check --write ./src",
    "start": "dotenv -- tsc && node dist/index.js",
    "dev": "dotenv -e .env.dev --  tsnd --respawn src/index.ts",
    "dev:local": "dotenv -e .env.local --  tsnd --respawn src/index.ts",
    "test": "dotenv -- jest",
    "test:local": "dotenv -e .env.local -- jest",
    "test:dev": "dotenv -e .env.dev -- jest",
    "migrate": "dotenv -- prisma migrate deploy --schema ./prisma/schema.prisma",
    "migrate:local": "dotenv -e .env.local -- prisma migrate dev --schema ./prisma/local/schema.local.prisma",
    "migrate:dev": "dotenv -e .env.dev -- prisma migrate dev --schema ./prisma/schema.prisma",
    "seed:local": "dotenv -e .env.local --  prisma db seed",
    "seed:dev": "dotenv -e .env.dev --  prisma db seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^5.19.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "4.19.2",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "supertest": "^7.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/bcrypt": "5.0.2",
    "@types/express": "4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "9.0.6",
    "@types/node": "22.4.0",
    "@types/supertest": "^6.0.2",
    "morgan": "1.10.0",
    "prisma": "^5.19.0",
    "rimraf": "6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
